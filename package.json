{
  "name": "backend-ironhealth",
  "version": "1.0.0",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "dev": "node --env-file=.env --watch app.js",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--no-warnings node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "seed": "node --env-file=.env api/seeds/adminSeed.js",
    "update-seed": "node --env-file=.env api/seeds/updateAdminSeed.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.1",
    "sanitize-html": "^2.17.0",
    "serverless-http": "^4.0.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^30.2.0",
    "lint-staged": "^15.2.0",
    "mongodb-memory-server": "^10.2.1",
    "prettier": "^3.2.4",
    "supertest": "^7.1.4"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
